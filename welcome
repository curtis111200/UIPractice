#include <iostream>

using namespace std;

int main(int argc, char *argv[])
{
	if(argc < 3 || argc > 3)
	{
		cout << "usage:" << endl;
		cout << "    " << argv[0] << " user" << endl;
		cout << "please enter argv[0],your name,master name" << endl;
	}
	else
	{
		cout << "Hello, " << argv[1] << "." << endl;
		cout << "Welcome to the " << argv[2] << "'s world." << endl;
	}
	
	return 0;
} 
